		class="edu.kit.anthropomatik.isl.newsTeller.retrieval.filtering.features.SimpleSparqlFeature">
		<constructor-arg name="queryFileName"
			value="resources/SPARQL/usability/hasActor.qry" />
		<property name="ksAdapter" ref="ksAdapter" />
		<property name="name" value="hasActor" />
	</bean>

