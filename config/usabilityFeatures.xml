	<bean id="hasActorFeature"
		class="edu.kit.anthropomatik.isl.newsTeller.retrieval.filtering.SimpleCountFeature">
		<constructor-arg name="queryFileName"
			value="resources/SPARQL/usability/hasActor.qry" />
		<constructor-arg name="probabilityFileName" value="" />
		<property name="ksAdapter" ref="ksAdapter" />
		<property name="name" value="hasActor" />
		<property name="possibleValues">
			<set>
				<value>0</value>
				<value>1</value>
				<value>2</value>
				<value>3</value>
				<value>4</value>
			</set>
		</property>
		<property name="defaultValue" value="4" />
	</bean>
	
