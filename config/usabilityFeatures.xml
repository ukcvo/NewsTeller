		class="edu.kit.anthropomatik.isl.newsTeller.retrieval.filtering.features.DBPediaLabelInFullTextFeature">
		<constructor-arg name="queryFileName"
			value="resources/SPARQL/usability/getEntities.qry" />
		<constructor-arg name="labelQueryFileName"
			value="resources/SPARQL/usability/getLabels.qry" />
		<property name="ksAdapter" ref="ksAdapter" />
