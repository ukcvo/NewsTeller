	<predicate lemma="price">
		<roleset framnet="-" id="price.01" name="assign/identify the price of" source="-" vncls="54.4">
			<roles>
				<role descr="seller, pricer" f="pag" n="0">
					<vnrole vncls="54.4" vntheta="agent"/>
				</role>
				<role descr="commodity" f="ppt" n="1">
