<!DOCTYPE frameset SYSTEM "frameset.dtd">
<frameset>
	<predicate lemma="transpose">
		<roleset framnet="-" id="transpose.01" name="move or switch (written) elements" source="" vncls="-">
			<roles>
				<role descr="transposer, agent" f="pag" n="0"/>
				<role descr="thing(s) transposed" f="ppt" n="1"/>
				<role descr="source, initial state/place" f="dir" n="2"/>
				<role descr="destination, final state/place" f="gol" n="3"/>
				
			
<note/>
</roles>
			<example name="Music" src="" type="">
				<text>Stravinsky transposed the National Anthem from C major to E flat major.</text>
				<arg f="" n="0">Stravinsky</arg>
				<rel f="">transposed</rel>
				<arg f="" n="1">the National Anthem</arg>
				<arg f="" n="2">from C major</arg>
				<arg f="" n="3">to E flat major</arg>
				
			
<note/>
</example>
			<example name="Orthography" src="" type="">
				<text>[The two letters]-1 were incorrectly transposed *trace*-1 in the word.</text>
				<arg f="mnr" n="m">incorrectly</arg>
				<rel f="">transposed</rel>
				<arg f="" n="1">*trace*-1</arg>
				<arg f="loc" n="m">in the word</arg>
				
			
<note/>
</example>
			
		
<note>No Vncls.</note>
<example name="two arg1s" src="BOLT" type="">
<inflection aspect="ns" form="ns" person="ns" tense="ns" voice="ns"/>
