SELECT ?event ?label
WHERE
{
    VALUES ?event { *keys* } .
    ?event sem:hasActor|sem:hasPlace ?entity .
    ?entity rdf:type* ?class .
    GRAPH <http://www.newsreader-project.eu/modules/dbpedia-en>
    {
        ?class rdfs:label ?labelEntity .
        ?class rdfs:comment ?label .
        FILTER(REGEX(STR(?labelEntity),"*k*", "i"))
    }
}