SELECT DISTINCT ?label
WHERE
{
    <*e*> sem:hasActor|sem:hasPlace ?entity .
    ?entity rdf:type* ?class .
    GRAPH <http://www.newsreader-project.eu/modules/dbpedia-en> # use dbpedia to get stable entities
    {
        ?class rdfs:label ?labelEntity .
        ?class rdfs:comment ?label .
        FILTER(REGEX(STR(?labelEntity),"*k*", "i")) #case-insensitive matching
    }
}