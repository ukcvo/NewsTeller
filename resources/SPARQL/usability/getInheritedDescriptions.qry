SELECT ?entity ?label
WHERE
{
    VALUES ?entity { *keys* } .
    ?entity rdf:type ?parent.
    ?parent rdf:type* ?class .
    GRAPH <http://www.newsreader-project.eu/modules/dbpedia-en> 
    {
        ?parent rdfs:comment ?label .
        ?class rdfs:label ?class_label .
        FILTER(REGEX(STR(?class_label),"*k*", "i")) #case-insensitive matching
    }
}