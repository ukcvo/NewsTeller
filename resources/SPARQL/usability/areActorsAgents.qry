SELECT ?event (AVG(?count) AS ?number)
WHERE
{
    SELECT ?event (COUNT(?link) AS ?count)
    WHERE
    {
        VALUES ?event { *keys* } .
        ?event sem:hasActor ?entity .
        OPTIONAL { ?entity ?link <http://dbpedia.org/ontology/Agent> }
    }
    GROUP BY ?entity ?event
}
GROUP BY ?event