SELECT ?event (MAX(?count) AS ?number)
WHERE
{
    SELECT ?event (COUNT(?label) AS ?count)
    WHERE
    {
        VALUES ?event { *keys* } .
        ?event sem:hasActor|sem:hasPlace ?entity .
        GRAPH <http://www.newsreader-project.eu/modules/dbpedia-en> 
        {
            ?entity rdfs:label ?label
        }
    }
    GROUP BY ?entity ?event
}
GROUP BY ?event