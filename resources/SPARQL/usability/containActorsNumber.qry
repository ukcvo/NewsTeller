SELECT ?event (MAX(?count) AS ?number)
WHERE
{
    SELECT ?event (COUNT(?label) AS ?count)
    WHERE
    {
        VALUES ?event { *keys* } .
        ?event sem:hasActor ?entity .
        OPTIONAL 
        {
            ?entity skos:prefLabel ?label .
            FILTER(REGEX(STR(?label),"[0-9]", "i"))
        }
    }
    GROUP BY ?entity ?event
}
GROUP BY ?event