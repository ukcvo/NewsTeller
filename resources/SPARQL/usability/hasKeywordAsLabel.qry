SELECT (COUNT(?label) AS ?number)
WHERE
{
    { <*e*> rdfs:label ?label }
    UNION
    { 
        <*e*> sem:hasActor|sem:hasPlace ?entity.
        ?entity rdf:type* ?class .
        GRAPH <http://www.newsreader-project.eu/modules/dbpedia-en>
        {
        ?class rdfs:label ?label
        }
    }
    FILTER(REGEX(STR(?label),"(-| |^|\\()*k*(\\w)*(-| |$|\\))", "i"))
}